        "dotenv": "^7.0.0",
        "express": "^4.16.4",
        "express-session": "^1.16.2",
        "express-validator": "^5.3.1",
        "lodash": "^4.17.14",
        "mongodb": "^3.2.2",
        "morgan": "^1.9.1",
        "multer": "^1.4.1",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "winston": "^3.2.1",
        "winston-daily-rotate-file": "^3.9.0"
    }
