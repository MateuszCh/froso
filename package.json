{
    "name": "froso",
    "version": "0.0.1",
    "description": "Headless Content Management System built on Express, MongoDB and React",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/**/*"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MateuszCh/froso.git"
    },
    "author": "Mateusz Chybiorz",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/MateuszCh/froso/issues"
    },
    "homepage": "https://github.com/MateuszCh/froso#readme",
    "scripts": {
        "build": "rm -rf lib && tsc",
        "dev": "ts-node src/server.ts",
        "dev-watch": "nodemon",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tslint -p tsconfig.json",
        "prepare": "npm run build",
        "prepublishOnly": "npm run lint"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.6",
        "@types/express-session": "^1.17.0",
        "@types/express-validator": "^3.0.0",
        "@types/lodash": "^4.14.149",
        "@types/mongodb": "^3.5.5",
        "@types/morgan": "^1.9.0",
        "@types/multer": "^1.4.2",
        "@types/node": "^13.11.1",
        "@types/passport": "^1.0.3",
        "@types/passport-local": "^1.0.33",
        "@types/winston": "^2.4.4",
        "nodemon": "^2.0.3",
        "prettier": "^2.0.4",
        "ts-node": "^8.8.2",
        "tslint": "^6.1.1",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.8.3",
        "typescript-tslint-plugin": "^0.5.5"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "connect-mongo": "^3.2.0",
        "cookie-parser": "^1.4.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-session": "^1.17.0",
        "express-validator": "^6.4.0",
        "lodash": "^4.17.15",
        "mongodb": "^3.5.5",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "winston": "^3.2.1",
        "winston-daily-rotate-file": "^4.4.2"
    }
}
