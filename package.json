{
    "name": "froso",
    "version": "0.0.1",
    "description": "Headless Content Management System built on Express, MongoDB and React",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/**/*"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MateuszCh/froso.git"
    },
    "author": "Mateusz Chybiorz",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/MateuszCh/froso/issues"
    },
    "homepage": "https://github.com/MateuszCh/froso#readme",
    "scripts": {
        "build": "rm -rf lib && tsc",
        "dev": "ts-node src/server.ts",
        "dev-watch": "nodemon",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tslint -p tsconfig.json",
        "prepare": "npm run build",
        "prepublishOnly": "npm run lint"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/connect-mongo": "0.0.41",
        "@types/cookie-parser": "^1.4.1",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^4.16.1",
        "@types/express-session": "^1.15.13",
        "@types/express-validator": "^3.0.0",
        "@types/lodash": "^4.14.123",
        "@types/mongodb": "^3.1.22",
        "@types/morgan": "^1.7.35",
        "@types/multer": "^1.3.7",
        "@types/node": "^11.13.0",
        "@types/passport": "^1.0.0",
        "@types/passport-local": "^1.0.33",
        "@types/winston": "^2.4.4",
        "nodemon": "^1.18.10",
        "prettier": "^1.16.4",
        "ts-node": "^8.0.3",
        "tslint": "^5.14.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.4.1",
        "typescript-tslint-plugin": "^0.3.1"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.18.3",
        "connect-mongo": "^2.0.3",
